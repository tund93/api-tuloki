{
  "$schema": "https://json.schemastore.org/swcrc",
   "jsc": {
     "parser": {
       "syntax": "typescript",
       "tsx": false,
       "decorators": true,
       "dynamicImport": true
     },
     "target": "es2021",
     "transform": {
       "legacyDecorator": true,
       "decoratorMetadata": true
     }
   },
   "module": {
     "type": "commonjs",
     "noInterop": false
   }
 }
